{
    "manifest_version": 2,
    "name": "New Tab Vue",
    "description": "A modular new tab grid page made in Vue.js",
    "version": "1.0",
    "permissions": [
        "<all_urls>",
        "geolocation"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}